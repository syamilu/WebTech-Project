{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "/style.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/style.css",
        "dest": "/style.css"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
}